func dark-search-and-replace(content) {
  content = text.replace(content, '>Dark<', '>Light<')
  content = text.replace(content, 'href="/', 'href="/dark/')
  content = text.replace(content, 'href="/dark/dark/dark', 'href="')
  return content
}
